public class Priest extends Protagonist{
    
   public Priest(){
	name = "Pressing Preist";
	health = 150;
	attack = 1;
	defense = 60;
	strength = (int) ( (Math.random() * 20) + 45 );
    }

    public Ogre_Priest( String entName ){
	this();
	name = entName;
    }  

    public static void about(){
	System.out.println("This class has low defense and low attack power and low health, but can heal itself every turn.");
    }

    public static void main (String[] args){
	about();
    }
}
